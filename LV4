<!-- Uvod u XML programiranje XPath-->
<!--
Zadaci rješenja
1. /Studenti/Student/Ime
2. /Studenti/Student/Predmet/Naslov
3. /Studenti/Student/Predmet/Naslov
4. /Studenti/Student[2]/Predmet/Vjezbe/Vjezba[text()="NE"]
5. sum(/Studenti/Student/Predmet/Kolokviji/Kolokvij)
6. /Studenti/Student[last()]/Prezime
7. /Studenti/Student[3]/contains(Ime,"a")
8. concat(/Studenti/Student[2]/Ime," ",/Studenti/Student[4]/Prezime)
9. 2025 - /Studenti/Student[@RedniBroj = 3]/GodinaRodjenja
10. boolean(/Studenti/Student/Predmet/Kolokviji/Kolokvij[text()="5"])
11. string-length(concat(/Studenti/Student[4]/Prezime,/Studenti/Student[2]/Ime))
12. count(/Studenti/Student[@RedniBroj="2"]/Predmet/Vjezbe/Vjezba[text()="NE"])
13. boolean(/Studenti/Student/Predmet/Kolokviji/Kolokvij[2][text()="0"])
14. /Studenti/Student[Grupa="A"]/Ime
15. /Studenti/Student[not(Predmet/Vjezbe/Vjezba="NE")]/Ime
16. /Studenti/Student[Predmet/Vjezbe/Vjezba="NE"]/Ime
17. /Studenti/Student/Predmet/Kolokviji/Kolokvij[text()<3]/@Sifra
18. /Studenti/Student[contains(Prezime,"ć")]/Ime
-->
